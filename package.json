{
  "name": "redirect-test",
  "version": "1.1.2",
  "description": "Test your web server redirects for the correct status code.",
  "main": "index.js",
  "repository": "git@github.com:cpitkin/redirect-test.git",
  "author": "Charlie Pitkin <charlie.pitkin@gmail.com>",
  "license": "MIT",
  "scripts": {
    "test": "nyc ava",
    "test-local": "ava -w",
    "lint": "eslint --fix ./index.js"
  },
  "bin": {
    "redirect-test": "index.js"
  },
  "devDependencies": {
    "ava": "^0.16.0",
    "babel-eslint": "^7.2.3",
    "eslint": "^4.3.0",
    "eslint-config-airbnb-base": "^11.3.1",
    "eslint-plugin-ava": "^4.2.1",
    "eslint-plugin-import": "^2.7.0",
    "execa": "^0.7.0",
    "nyc": "^10.0.0",
    "restify": "^4.3.0"
  },
  "dependencies": {
    "async": "^3.2.2",
    "chalk": "^1.1.3",
    "commander": "^2.9.0",
    "csv": "^1.1.1",
    "is-absolute-url": "^2.1.0",
    "ora": "^1.3.0",
    "request": "^2.81.0",
    "underscore": "^1.8.3",
    "write-csv": "^1.0.0"
  },
  "ava": {
    "files": [
      "test/**.test.js",
      "test/*.csv"
    ],
    "timeout": "10s",
    "failFast": false,
    "failWithoutAssertions": true,
    "powerAssert": true
  },
  "nyc": {
    "check-coverage": true,
    "lines": 90,
    "functions": 100,
    "branches": 90,
    "statements": 90,
    "include": [
      "index.js"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "cache": true,
    "all": true
  }
}
